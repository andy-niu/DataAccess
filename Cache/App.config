<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--↓↓↓↓Redis相关配置↓↓↓↓-->
    <!--Redis服务器集合（使用“,”隔开）  192.168.100.152:6379-->
    <add key="RedisWriteServers" value="127.0.0.1:6379"/>
    <add key="RedisReadServers" value="127.0.0.1:6379"/>
    <!--Redis服务器名称-->
    <add key="RedisWriteSentinelName" value="WriteMaster"/>
    <add key="RedisReadSentinelName" value="ReadMaster"/>
    <!--Redis连接池读写最大连接数-->
    <add key="RedisPoolMaxWrite" value="100"/>
    <!--Redis连接池只读最大连接数-->
    <add key="RedisPoolMaxRead" value="100"/>
    <!--↑↑↑↑Redis相关配置↑↑↑↑-->
  </appSettings>
</configuration>